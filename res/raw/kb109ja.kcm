    ctrl, alt, meta:                    none
}

key YEN {
    label, number:                      '\u00a5'
    base:                               '\u00a5'
    shift:                              '|'
    alt, ctrl, meta:                    none
}

key ESCAPE {
    base:                               fallback BACK
    meta:                               fallback HOME
    alt:                                fallback MENU
}
